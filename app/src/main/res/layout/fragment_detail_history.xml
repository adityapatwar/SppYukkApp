<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingStart="@dimen/padding12dp"
    android:paddingEnd="@dimen/padding12dp"
    tools:context=".ui.detail.history.fragment.DetailHistoryFragment">

    <ImageView
        android:id="@+id/image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/image"
        android:src="@drawable/ic_detauk_trans"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.05" />

    <TextView
        android:id="@+id/nameT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Name"
        android:textSize="@dimen/text16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/image"
        app:layout_constraintVertical_bias="0.075" />

    <TextView
        android:id="@+id/name"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:maxLines="1"
        android:textSize="@dimen/text16sp"
        app:layout_constraintBottom_toBottomOf="@id/nameT"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.8"
        app:layout_constraintStart_toEndOf="@id/nameT"
        app:layout_constraintTop_toTopOf="@id/nameT"
        app:layout_constraintWidth_percent="0.5"
        tools:text="Ujang ujang Sapeudin Noah" />


    <TextView
        android:id="@+id/nisnT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/nisn"
        android:textSize="@dimen/text16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/nameT"
        app:layout_constraintVertical_bias="0.075" />

    <TextView
        android:id="@+id/nisn"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:maxLines="1"
        android:textSize="@dimen/text16sp"
        app:layout_constraintBottom_toBottomOf="@id/nisnT"
        app:layout_constraintEnd_toEndOf="@id/name"
        app:layout_constraintHorizontal_bias="0.8"
        app:layout_constraintStart_toStartOf="@id/name"
        app:layout_constraintTop_toTopOf="@id/nisnT"
        app:layout_constraintWidth_percent="0.5"
        tools:text="1234567890" />


    <TextView
        android:id="@+id/jumlahT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/jumlah_bayar"
        android:textSize="@dimen/text16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/nisnT"
        app:layout_constraintVertical_bias="0.075" />

    <TextView
        android:id="@+id/jumlah"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:maxLines="1"
        android:textSize="@dimen/text16sp"
        app:layout_constraintBottom_toBottomOf="@id/jumlahT"
        app:layout_constraintEnd_toEndOf="@id/name"
        app:layout_constraintHorizontal_bias="0.8"
        app:layout_constraintStart_toStartOf="@id/name"
        app:layout_constraintTop_toTopOf="@id/jumlahT"
        app:layout_constraintWidth_percent="0.5"
        tools:text="Rp 100,000" />

    <TextView
        android:id="@+id/tunggakT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tunggak"
        android:textSize="@dimen/text16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/jumlahT"
        app:layout_constraintVertical_bias="0.075" />

    <TextView
        android:id="@+id/tunggak"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:maxLines="1"
        android:textSize="@dimen/text16sp"
        app:layout_constraintBottom_toBottomOf="@id/tunggakT"
        app:layout_constraintEnd_toEndOf="@id/name"
        app:layout_constraintHorizontal_bias="0.8"
        app:layout_constraintStart_toStartOf="@id/name"
        app:layout_constraintTop_toTopOf="@id/tunggakT"
        app:layout_constraintWidth_percent="0.5"
        tools:text="Rp 1,000,000" />


    <TextView
        android:id="@+id/dateT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/date"
        android:textSize="@dimen/text16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tunggakT"
        app:layout_constraintVertical_bias="0.075" />

    <TextView
        android:id="@+id/date"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:maxLines="1"
        android:textSize="@dimen/text16sp"
        app:layout_constraintBottom_toBottomOf="@id/dateT"
        app:layout_constraintEnd_toEndOf="@id/name"
        app:layout_constraintHorizontal_bias="0.8"
        app:layout_constraintStart_toStartOf="@id/name"
        app:layout_constraintTop_toTopOf="@id/dateT"
        app:layout_constraintWidth_percent="0.5"
        tools:text="1/12/2019" />

    <TextView
        android:id="@+id/sppT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/petugas"
        android:textSize="@dimen/text16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/dateT"
        app:layout_constraintVertical_bias="0.075" />

    <TextView
        android:id="@+id/petugas"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:maxLines="1"
        android:textSize="@dimen/text16sp"
        app:layout_constraintBottom_toBottomOf="@id/sppT"
        app:layout_constraintEnd_toEndOf="@id/name"
        app:layout_constraintHorizontal_bias="0.8"
        app:layout_constraintStart_toStartOf="@id/name"
        app:layout_constraintTop_toTopOf="@id/sppT"
        app:layout_constraintWidth_percent="0.5"
        tools:text="Tahun 2019" />


    <androidx.cardview.widget.CardView
        android:id="@+id/createReport"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin16dp"
        android:layout_marginBottom="@dimen/margin16dp"
        android:clickable="true"
        android:focusable="true"
        android:foreground="?attr/selectableItemBackground"
        app:cardBackgroundColor="@color/colorPrimary"
        app:cardCornerRadius="2dp"
        app:cardElevation="0dp"
        app:contentPadding="6dp"
        app:contentPaddingLeft="@dimen/padding12dp"
        app:contentPaddingRight="@dimen/padding12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.98"
        app:layout_constraintStart_toStartOf="parent">

        <TextView
            android:id="@+id/textOne"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/buat_Laporan"
            android:textAlignment="center"
            android:textColor="@android:color/white"
            android:textSize="@dimen/text16sp"
            android:textStyle="bold" />
    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>